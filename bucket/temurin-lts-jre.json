{
    "description": "Eclipse Temurin is a runtime provided by Eclipse Adoptium for general use across the Java ecosystem",
    "homepage": "https://adoptium.net",
    "version": "17.0.8-7",
    "license": "GPL-2.0-only WITH Classpath-exception-2.0",
    "architecture": {
        "64bit": {
            "url": "https://github.com/adoptium/temurin17-binaries/releases/download/jdk-17.0.8+7/OpenJDK17U-jre_x64_windows_hotspot_17.0.8_7.zip",
            "hash": "216aa7d4db4bd389b8e3d3b4f1a58863666c37c58b0a83e1c744620675312e36"
        },
        "32bit": {
            "url": "https://github.com/adoptium/temurin17-binaries/releases/download/jdk-17.0.8+7/OpenJDK17U-jre_x86-32_windows_hotspot_17.0.8_7.zip",
            "hash": "e8ed347915ba0badf605e94dea0f9b266b64baced152e09d663dd6b8b60ca19d"
        }
    },
    "extract_dir": "jdk-17.0.8+7-jre",
    "env_add_path": "bin",
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "checkver": {
        "url": "https://api.adoptium.net/v3/assets/version/%5B8%2C%29?release_type=ga&lts=true&jvm_impl=hotspot&heap_size=normal&os=windows&architecture=x64&image_type=jre&project=jdk&vendor=eclipse&page_size=1&sort_order=DESC",
        "script": [
            "$ver = (json_path $page $..version_data.semver).replace('+', '-')",
            "$link = (json_path $page $..release_link).replace('%2B', '+')",
            "$name = json_path $page $..binaries[0].package.name",
            "Write-Output \"$ver $link $name\""
        ],
        "regex": "(?<ver>.*?) https://github.com/(?<link>.*?)/tag/(?<tag>.*?) (?<name>(?<prefix>OpenJDK[\\dU]*-[dejkr]{3})_x64_(?<suffix>.*))",
        "replace": "${ver}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/$matchLink/download/$matchTag/$matchPrefix_x64_$matchSuffix",
                "hash": {
                    "url": "$url.sha256.txt",
                    "find": "^([a-fA-F0-9]+)\\s"
                }
            },
            "32bit": {
                "url": "https://github.com/$matchLink/download/$matchTag/$matchPrefix_x86-32_$matchSuffix",
                "hash": {
                    "url": "$url.sha256.txt",
                    "find": "^([a-fA-F0-9]+)\\s"
                }
            }
        },
        "extract_dir": "$matchTag-jre"
    }
}
